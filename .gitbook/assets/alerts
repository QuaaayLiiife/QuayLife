    if (window.location.pathname.substr(0, 3) == '/v4' && (window.location.pathname.length > 4 || window.location.hash != '#/changesummary')) {
        window.location.replace('/v4/#/changesummary');
    }
    // Redirecting from implicit default version to explicit default version
    
    else if (window.location.pathname.substr(0, 3) != '/v7') {
        window.location.replace('/v7' + window.location.pathname + window.location.hash);
    }
    
</script>

